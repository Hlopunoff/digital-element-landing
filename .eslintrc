{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "linebreak-style": 0,
        "global-require": 0,
        "eslint linebreak-style": [0, "error", "windows"],
        "import/prefer-default-export": "off",
        "no-restricted-syntax": "off",
        "no-control-regex": "off",
        "consistent-return": "off",
        "max-classes-pre-file": false
    }
}